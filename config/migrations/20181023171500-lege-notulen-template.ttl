@prefix rdf:	<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ns1:	<http://mu.semte.ch/vocabularies/ext/> .
@prefix ns2:	<http://mu.semte.ch/vocabularies/core/> .
@prefix ns3:	<http://data.vlaanderen.be/ns/besluit#> .
@prefix ns4:	<http://data.vlaanderen.be/ns/mandaat#> .
@prefix ns5:	<http://purl.org/dc/terms/> .
@prefix foaf:	<http://xmlns.com/foaf/0.1/> .

<http://lblod.info/templates/58c6d263-7b2e-477b-a1eb-90379e9e5560>
    rdf:type	ns1:Template ;
    ns2:uuid	"58c6d263-7b2e-477b-a1eb-90379e9e5560" ;
    ns1:templateMatches "notulen";
    ns1:activeInContext	foaf:Document ;
    ns1:disabledInContext	ns3:Zitting ;
    ns5:title	"Lege notulen" ;
    ns1:templateContent """<div typeof="besluit:Zitting" resource="http://data.lblod.info/zittingen/${generateUuid()}">
  <div class="annotation-structure">

    <!-- meta zitting -->
    <h2 class="h1" property="dc:title">
      Notulen van <span property="ext:scopedBestuursorgaanText">selecteer bestuursorgaan</span> , zitting van <span class="annotation" property="besluit:geplandeStart" datatype="xsd:date" content=""><span typeOf="ext:currentDate">&nbsp;</span></span>â€‹
    </h2>
    </p><p>
      van <span id="${generateBoundUuid('startAtTime-1')}" property="prov:startedAtTime" content="" datatype="xsd:dateTime" class="annotation"><span typeOf="ext:currentDate">&nbsp;</span></span> tot <span property="prov:endedAtTime" content="" datatype="xsd:dateTime" class="annotation"><span typeOf="ext:currentDate">&nbsp;</span></span></p>
  </div>
  <br>

  <!-- opening -->
  <div class="annotation-structure">
    <h3 class="h2">Opening</h3>
    <p>De gemeenteraad-voorzitter verklaart de zitting open om 19:00 uur en deelt mee dat er een opname gemaakt wordt.</p>
  </div>
  <br>

  <!-- aanwezigen-->
  <div class="annotation-structure">
    <h3 class="h2">Aanwezigen</h3>
    <ul class="bullet-list">
      <li class="u-spacer--small"> </li>
    </ul>
  </div>
  <br>

  <!-- Agenda overzicht-->
  <div class="annotation-structure" property="besluit:heeftAgenda" resource="http://data.lblod.info/agendas/${generateUuid()}" typeof="besluit:Agenda">
    <h3 class="h2">Agenda overzicht</h3>

    <h4 class="h3" property="http://mu.semte.ch/vocabularies/ext/noHighlight" typeof="http://mu.semte.ch/vocabularies/ext/NoHighlight">Open agendapunten</h4>
    <ul class="icon-list">
      <li>agendapunt</li>
    </ul>
    <br>

    <h4 class="h3" property="http://mu.semte.ch/vocabularies/ext/noHighlight" typeof="http://mu.semte.ch/vocabularies/ext/NoHighlight">Besloten agendapunten</h4>
    <ul class="icon-list">
      <li>agendapunt</li>
    </ul>
    <br>
  </div>
  <br>

  <!-- Agenda behandeling -->
  <div class="annotation-structure">
    <h3 class="h2">Agendapunten</h3>
    <h4 class="h3">Open zitting</h4>
    <h4 class="h3">Besloten zitting</h4>
  </div>
  <br>

  <!-- Sluiting zitting-->
  <div class="annotation-structure">
    <h3 class="h2">Sluiting van de zitting</h3>
    <p>De voorzitter sluit de zitting.</p>
  </div>
  <br>

  <!-- Authenticatie -->
  <div class="grid u-spacer--top">
    <div class="col--6-12" style="height: 20rem;">
      <p>&lt;Mandataris&gt;</p>
      <p>Stadssecretaris</p>
    </div>
    <div class="col--6-12" style="height: 20rem;">
      <p>&lt;Mandataris&gt;</p>
      <p>Voorzitter</p>
    </div>
  </div>
</div>
""".