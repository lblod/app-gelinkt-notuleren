PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>


WITH <http://mu.semte.ch/application>

DELETE {
        ?s ?p ?oldURI .
        }
INSERT {
         ?s ?p ?newURI .
}
WHERE {
       ?oldURI rdf:type besluit:Besluit .
       ?s ?p ?oldURI .
       BIND (IRI(CONCAT(STR(?oldURI), '/volledigdocument'))
             AS ?newURI) .

       FILTER ( REGEX(?oldURI, "codex.opendata" )) .
       FILTER ( !REGEX(?oldURI, "volledigdocument" )) .
       };

DELETE {
        ?oldURI ?p ?o .
        ?oldURI rdf:type besluit:Besluit .
        }
INSERT {
         ?newURI ?p ?o .
         ?newURI rdf:type besluit:Besluit .
}
WHERE {
       ?oldURI rdf:type besluit:Besluit .
       ?oldURI ?p ?o .
       BIND (IRI(CONCAT(STR(?oldURI), '/volledigdocument'))
             AS ?newURI) .

       FILTER ( REGEX(?oldURI, "codex.opendata" )) .
       FILTER ( !REGEX(?oldURI, "volledigdocument" )) .
       };
